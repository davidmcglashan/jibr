[
    {
        "name": "System Commands",
        "commands": {
            "help":     "jbHelp.help",
            "echo":     "jbEcho.echof",
            "quit": {
                "function": "jbFunc.quit",
                "alias":    "q"
            },
            "print": {
                "function": "jbPrint.printf",
                "alias":    "p"
            }
        }
    },

    {
        "name": "Jira host configuration and authentication",
        "commands": {
            "hostname": "jbHost.hostnamef",
            "port":     "jbHost.portf",
            "host":     "jbHost.hostf",
            "token":    "jbHost.token",
            "jira":     "jbHost.jiraf"
        }
    },

    {
        "name": "Executing a script file",
        "commands": {
            "script": {
                "function": "jbScript.script",
                "alias":    "x"
            }
        }
    },

    {
        "name": "Records, fields and columns",
        "commands": {
            "record":   "jbRecord.recordf",
            "field":    "jbField.fieldf",
            "fields":   "jbFields.getf"
        }
    },

    {
        "name": "Quick searching with JQL",
        "commands": {
            "search": {
                "function": "jbSearch.searchf",
                "alias":    "?"
            },
            "startat":      "jbSearch.startAtf",
            "maxresults":   "jbSearch.maxResultsf",
            "select":       "jbSelect.columnsf"
        }
    },

    {
        "name": "Analysing payloads",
        "commands": {
            "payload":  "jbPayload.payloadf",
            "look":     "jbLook.lookf",
            "csv":      "jbCsv.csvf",
            "count":    "jbCount.countf",
            "bucket": {
                "function": "jbBucket.bucketf",
                "alias":    "bk" 
            },
            "worklogs": "jbWorklog.parsef"
        }
    },

    {
        "name": "Working with variables",
        "commands": {
            "var": "jbVar.varf",
            "array": {
                "function": "jbArray.arrayf",
                "alias":    "arr"
            }
        }
    }
]